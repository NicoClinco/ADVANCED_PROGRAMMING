#include <pybind11/pybind11.h>
#include <pybind11/stl.h>
#include "data_frame.hpp"


namespace py = pybind11;


PYBIND11_MODULE(pyDataframe, m) {
  m.doc() = "pybind11 example plugin"; // optional module docstring

  py::class_<CSV_READER::dataframe>(m,"pyDataFrame")
    .def(py::init<const std::string&>())
    .def("Read",&CSV_WRITER::MyIterable::Read,"Read the file specified by the user")
    .def_read("filename_",&CSV_WRITER::MyIterable:filename_,"Read the filename")
}
